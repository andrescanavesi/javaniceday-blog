<!doctype html>
<html lang="en">
  <head>

    <link rel="preconnect" href="//stackpath.bootstrapcdn.com" crossorigin>
    <link rel="preconnect" href="https://res.cloudinary.com" crossorigin>

    {{#if isProduction}}
      <!--Global site tag (gtag.js) - Google Analytics-->
      <link rel="preconnect" href="//googletagmanager.com" crossorigin>
      <script async="" src="https://www.googletagmanager.com/gtag/js?id=UA-156358716-1"></script>
      <script>
          window.dataLayer = window.dataLayer || [];

          function gtag() {
              dataLayer.push(arguments);
          }
          gtag('js', new Date());
          gtag('config', 'UA-156358716-1');
      </script>

      {{#if adsenseEnabled}}
	  <!-- Adsense -->
	  <script data-ad-client="ca-pub-9559827534748081" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
      {{/if}}
